@{
    Layout = null;
    Response.ContentType = "text/xml";
}
@model IEnumerable<Shedule.Models.Events>
<data>
    @foreach (var myevent in Model)
    {
        <event id="@myevent.id">
            <start_date><![CDATA[@String.Format("{0:dd/MM/yyyy HH:mm}", myevent.start_date)]]></start_date>
            <end_date><![CDATA[@String.Format("{0:dd/MM/yyyy HH:mm}", myevent.end_date)]]></end_date>
            <text><![CDATA[@myevent.text]]></text>
            <type><![CDATA[@myevent.type]]></type>
            <group><![CDATA[@myevent.group]]></group>
            <subject><![CDATA[@myevent.subject]]></subject>
            <teacher><![CDATA[@myevent.teacher]]></teacher>
            <cabinet><![CDATA[@myevent.cabinet]]></cabinet>
        </event>
    }
</data>
